{
  "editor.linkedEditing": true,
  "editor.tabSize": 2,
  "git.autofetch": true,
  "git.confirmSync": false,
  "yaml.schemas": {
    "packages/infra/Pulumi.config.schema.json": "Pulumi.*.yaml",
    "./.moon/cache/schemas/workspace.json": [".moon/workspace.yml"],
    "./.moon/cache/schemas/tasks.json": [
      ".moon/tasks.yml",
      ".moon/tasks/**/*.yml"
    ],
    "./.moon/cache/schemas/project.json": ["**/moon.yml"],
    "./.moon/cache/schemas/toolchain.json": [".moon/toolchain.yml"],
    "./.moon/cache/schemas/template.json": ["**/template.yml"],
    ".azuredevops/AzurePipelines.schema.json": [".azuredevops/pipelines/*.yaml"]
  },
  "json.schemas": [
    {
      "url": "https://aka.ms/azure/static-web-apps-cli/schema",
      "fileMatch": ["swa-cli.config.json"]
    },
    {
      "url": "https://json.schemastore.org/staticwebapp.config.json",
      "fileMatch": ["staticwebapp.config.json"]
    }
  ],
  "moon.binPath": "~/.moon/bin/moon",
  "prettier.enable": true,
  "prettier.configPath": ".prettierrc.json",
  "prettier.useEditorConfig": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
